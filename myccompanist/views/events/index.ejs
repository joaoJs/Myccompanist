<h2 class="myProfile"> Events </h2>



<div class="new-list-group">
  <% events.forEach(event => { %>
    <div class="new-list-group-item-message">
      <a href="/events/<%=event._id%>/info" class="titleLink"> <%= event.title %> </a>
      <br>
      <div class="evPs">
      <p> Creator: <%= event.creator %> </p>
      <br>
      <p> When: <%= event.date %> </p>
      <br>
      <p> Instruments needed: <%= event.requiredInstruments %> </p>
      </div>
      <br>



    </div>
  <% }) %>

</div>
